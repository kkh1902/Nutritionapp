<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="192px"
        android:height="512px">
        <shape
            android:shape="rectangle">
            <solid android:color="@color/white" />
        </shape>
    </item>
    <item
        android:width="128px"
        android:height="512px"
        android:left="0px">
        <shape
            android:shape="rectangle">
            <solid android:color="@color/blue" />
        </shape>
    </item>
    <item
        android:width="128px"
        android:height="512px"
        android:left="0px">
        <shape
            android:shape="rectangle">
            <solid android:color="@color/white" />
            <corners android:bottomRightRadius="128px" />
        </shape>
    </item>
    <item
        android:width="64px"
        android:height="512px"
        android:left="128px">
        <shape
            android:shape="rectangle">
            <solid android:color="@color/blue" />
            <corners android:topLeftRadius="64px" />
        </shape>
    </item>
</layer-list>